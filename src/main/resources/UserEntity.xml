<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="org.models.UserEntity">
        <table name="user" schema="public" catalog="citizendefender"/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="name">
                <column name="name" length="50"/>
            </basic>
            <basic name="email">
                <column name="email" length="60"/>
            </basic>
            <basic name="password">
                <column name="password" length="100"/>
            </basic>
            <basic name="cin">
                <column name="cin" nullable="false" precision="-1"/>
            </basic>
            <basic name="photopath">
                <column name="photopath" nullable="false"/>
            </basic>
            <basic name="couverturepath">
                <column name="couverturepath" nullable="false"/>
            </basic>
            <basic name="isadmin">
                <column name="isadmin" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
